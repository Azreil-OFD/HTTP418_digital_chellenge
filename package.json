    "@nuxtjs/tailwindcss": "^6.12.2",
    "@pinia/nuxt": "^0.7.0",
    "@primevue/nuxt-module": "^4.2.1",
    "nuxt": "^3.11.2",
    "vue": "^3.4.23",
    "vue-router": "^4.3.2"
  },
  "dependencies": {
    "@primevue/forms": "^4.2.1",
    "@primevue/themes": "^4.2.1",
    "concurrently": "^8.2.2",
    "pinia": "^2.2.6",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.1",
    "zod": "^3.23.8"
  }
}
